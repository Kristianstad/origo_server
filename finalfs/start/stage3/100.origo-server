# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
   cd "$VAR_CONFIG_DIR"
   find * -type d -exec mkdir -p "/origo-server/{}" \;
   find * -type f -exec ln -sf "$VAR_CONFIG_DIR/{}" "/origo-server/{}" \;
   #replaceDirWithLink "/origo-server/conf" "$VAR_CONFIG_DIR"
fi
